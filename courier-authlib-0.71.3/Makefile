authldap.ldif: authldap.schema
	echo 'dn: cn=courier,cn=schema,cn=config' >authldap.ldif.tmp
	echo 'objectClass: olcSchemaConfig' >>authldap.ldif.tmp
	echo 'cn: courier' >>authldap.ldif.tmp
	sed '/^#/d;s/^attributetype/olcAttributeTypes:/;s/^objectclass/olcObjectClasses:/;s/^$$/#/' <$(srcdir)/authldap.schema | tr '\011' ' ' >>authldap.ldif.tmp
	mv -f authldap.ldif.tmp authldap.ldif

README_authlib.html: README_authlib.html.in config.status
	$(SHELL) ./config.status --file=README_authlib.html

authdaemond.o: authdaemonrc.h authdaemond.c

install-migrate: authmigrate
	/bin/sh authmigrate

install-configure:
	test -f $(DESTDIR)/test/etc/authlib/authdaemonrc.dist && /bin/perl ././sysconftool $(DESTDIR)/test/etc/authlib/authdaemonrc.dist || true
	test -f $(DESTDIR)/test/etc/authlib/authmysqlrc.dist && /bin/perl ././sysconftool $(DESTDIR)/test/etc/authlib/authmysqlrc.dist || true
	test -f $(DESTDIR)/test/etc/authlib/authpgsqlrc.dist && /bin/perl ././sysconftool $(DESTDIR)/test/etc/authlib/authpgsqlrc.dist || true
	test -f $(DESTDIR).dist && /bin/perl ././sysconftool $(DESTDIR).dist || true
	test -f $(DESTDIR)/test/etc/authlib/authsqliterc.dist && /bin/perl ././sysconftool $(DESTDIR)/test/etc/authlib/authsqliterc.dist || true

# For maintainer's use only

distrelease:
	umask 022; $(MAKE) distcheck
	$(MAKE) rpm
	$(MAKE)
	$(MAKE)
	rm -f courier-authlib.spec
	$(MAKE) courier-authlib.spec
	$(MAKE) dist

rpm: rpm-build

rpm-build:
	$(MAKE) bump.rpm.release
	$(MAKE) dist
	$(MAKE) dorpm

publish: publish-

publish-dev:
	$(MAKE) dopublish SUBDIR=dev

publish-prod:
	$(MAKE) dopublish SUBDIR=prod

dopublish:
	lpbuild --resign lpm/*
	rm -rf lpmtmp
	mkdir lpmtmp
	ln lpm/* lpmtmp
	rm -f lpmtmp/*debuginfo*
	mv lpmtmp/*.src.lp repository-$(SUBDIR)/`lpm -q --distribution`/sources
	mv lpmtmp/*.lp repository-$(SUBDIR)/`lpm -q --distribution`/packages
	rm -rf lpmtmp
	ls -t repository-$(SUBDIR)/`lpm -q --distribution`/sources | tail -n +6 | xargs -r -n 1 lpm --erase repository-$(SUBDIR)
	lpm --sync repository-$(SUBDIR)
	rsync -a -v --delete-after repository-$(SUBDIR)/`lpm -q --distribution`/. headache:lpmtool.com/www/repositories/$(SUBDIR)/courier-authlib/`lpm -q --distribution`

dorpm:
	rm -rf rpm/BUILD/*
	rm -f rpm/RPMS/*/*
	rm -f rpm/SOURCES/*
	rm -f rpm/SPECS/*
	rm -f rpm/SRPMS/*
	rpmbuild -ta --clean \
		--define "_topdir `pwd`/rpm" \
		--define '_rpmdir %{_topdir}/RPMS' \
		--define '_srcrpmdir %{_topdir}/SRPMS' \
		--define '_sourcedir %{_topdir}/SOURCES' \
		--define '_specdir %{_topdir}/SPECS' \
		--define '_builddir %{_topdir}/BUILD' \
		--define '_build_name_fmt %%{ARCH}/%%{NAME}-%%{VERSION}-%%{RELEASE}.%%{ARCH}.rpm' \
		--define '_tmppath %{_var}/tmp' \
		--define '__spec_prep_pre %{___build_pre}' \
		courier-authlib-0.71.3.tar.bz2

bump.rpm.release: courier-authlib.spec
	test -f $(srcdir)/rpm.release && . $(srcdir)/rpm.release; \
	NEXT_RELEASE=100; \
	test "$$VERSION" = "0.71.3" && NEXT_RELEASE="$$RELEASE"; \
	RELEASE=`expr $$NEXT_RELEASE + 1`; \
	echo "VERSION=0.71.3" >$(srcdir)/rpm.release; \
	echo "RELEASE=$$RELEASE" >>$(srcdir)/rpm.release; \
	sed 's/^Release: .*/Release: '$$RELEASE'%{?dist}%{?courier_release}/' \
		<courier-authlib.spec >courier-authlib.spec.new; \
	mv courier-authlib.spec.new courier-authlib.spec;

upload-beta:
	rm -f courier-authlib-0.71.3.tar.bz2.sig; gpg --detach-sign --default-key mrsam@courier-mta.com courier-authlib-0.71.3.tar.bz2; test -f courier-authlib-0.71.3.tar.bz2.sig || exit 1
	~/src/sftools/release.pl mrsam courier authlib-devel `echo 0.71.3|sed 's/.*\(........\)/\1/'` courier-authlib-0.71.3.tar.bz2 courier-authlib-0.71.3.tar.bz2.sig

upload-dist:
	rm -f courier-authlib-0.71.3.tar.bz2.sig; gpg --detach-sign --default-key mrsam@courier-mta.com courier-authlib-0.71.3.tar.bz2; test -f courier-authlib-0.71.3.tar.bz2.sig || exit 1
	set -e; v="courier-authlib/0.71.3/`date +%Y%m%d%H%M%S`"; git tag -s $$v -m 'Tag version 0.71.3'; cd libs; git tag -s $$v -m 'Tag version 0.71.3'
	socksify ~/src/sftools/release.pl mrsam courier authlib 0.71.3 courier-authlib-0.71.3.tar.bz2 courier-authlib-0.71.3.tar.bz2.sig

htmldoc: $(HTMLDOCFILES)
	for f in $^; do d=`basename $$f`; test "$$d" = "README.html" && d="index.html"; tidy -i -q --tidy-mark no --doctype transitional --input-encoding utf8 --output-encoding utf8 -asxml <$$f >$$f.tmp ; xsltproc --nonet $$HOME/www/www.courier-mta.org/xsl/authlib.xsl $$f.tmp | perl -ne 's~\$(id):.*\$$~\$(id):'"$$h"'\$$~; print' >$$HOME/www/www.courier-mta.org/authlib/$$d; rm -f $$f.tmp; done

